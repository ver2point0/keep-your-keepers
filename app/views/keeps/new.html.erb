<h1>New Keep</h1>
<div class="row">
  <div class="col-md-4">
    <%= form_for @keep do |f| %>
      <div class="form-group">
        <%= f.label :title %> (minimum of 1 character)
        <%= f.text_field :title, class: "form-control", placeholder: "Enter keep title" %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: "btn btn-success" %>
      </div>
    <% end %>
  </div>
</div>